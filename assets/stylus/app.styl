    &.cms
        background-color: #f8f8fb
    &.no_click
        pointer-events: none
    &.no_scroll
        overflow-y: hidden

*,
*:before,
*:after
    user-select: none
    box-sizing: border-box
    margin: 0

input,
input:before,
input:after,
textarea,
textarea:before,
textarea:after,
select,
select:before,
select:after
    user-select: initial

h1,
h2,
h3,
h4,
h5
