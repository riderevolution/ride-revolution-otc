img
    height: auto
    vertical-align: middle

table
    width: 100%
    border-collapse: collapse

.apexcharts-toolbar
    z-index: 1

.apexcharts-menu-item
    sf_pro_regular(14px)

.hidden
    display: none

.none
    pointer-events: none

.scroll
    height: 100%
    max-height: 800px
    overflow-y: auto
    margin-bottom: 30px
    .cms_table
        margin-bottom: 0

@page
    size: landscape
    margin: 0cm

@media print
    body
        margin: 0.75cm
        -webkit-print-color-adjust: exact

.cms_table_scroll
