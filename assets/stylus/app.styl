/**
 * Booker **/
.booker_wrapper
    display: flex
    flex-flow: row wrap
    align-items: flex-start
    justify-content: space-between
    margin-bottom: 60px
    .booker_button
        flex: 0 0 auto
    .booker_classes
        flex: 0 0 25%
        background-color: $white
        opacity: 1
        transition: .3s ease-in-out
        &.disable_booker
            opacity: 0.5
            pointer-events: none
            transition: .3s ease-in-out
        .header_wrapper
            display: flex
            flex-flow: row wrap
            align-items: center
            justify-content: space-between
            padding: 15px 10px
            border-bottom: 1px solid $border
            .booker_header
                position: relative
                display: flex
                flex-flow: row wrap
                align-items: center
                .booker_title
                    sf_pro_bold(16px)
                    color: $black
                    margin: 0 15px
                .booker_prev
                    transform: rotate(180deg)
                .booker_next,
                .booker_prev
                    width: 28px
                    height: 28px
                    svg
                        .arrow_1
                            fill: $white
                            stroke: $border
                            transition: .3s ease-in-out
                        .arrow_2,
                        .arrow_4
                            fill: none
                        .arrow_2
                            stroke: $green
                            stroke-width: 1.5px
                        .arrow_3
                            stroke: none
                        &:hover
                            .arrow_1
                                fill: $border
                                transition: .3s ease-in-out
        .content_wrapper
            .class_accordion
                &.highlighted
                    .accordion_header
                        color: $orange
                        border-bottom: 1px solid $orange
                        transition: .3s ease-in-out
                &.toggled
                    .accordion_header
                        color: $green
                        border-bottom: 1px solid $green
                        transition: .3s ease-in-out
                        &::before
                            transform: translateY(-50%) rotate(-180deg)
                            transition: .3s ease-in-out
                    .accordion_content
                        opacity: 1
                        overflow: initial
                        visibility: visible
                        transition: .3s ease-in-out
                    &.highlighted
                        .accordion_header
                            color: $orange
                            border-bottom: 1px solid $orange
                            transition: .3s ease-in-out
                .accordion_header
                    position: relative
                    padding: 15px
                    sf_pro_semibold(16px)
                    color: $black
                    cursor: pointer
                    border-bottom: 1px solid $border
                    transition: .3s ease-in-out
                    &:hover
                        color: $green
                        border-bottom: 1px solid $green
                        transition: .3s ease-in-out
                    &::before
                        content: url(/icons/custom-select-icon.svg)
                        position: absolute
                        top: 50%
                        right: calc(0% + 15px)
                        transform: translateY(-50%) rotate(0deg)
                        transition: .3s ease-in-out
                .accordion_content
                    height: 0
                    opacity: 0
                    overflow: hidden
                    visibility: hidden
                    transition: .3s ease-in-out
                    .no_class
                        sf_pro_regular(16px)
                        color: $black
                        padding: 15px 20px
                        text-align: center
                    .class_content
                        display: block
                        padding: 10px 15px
                        text-decoration: none
                        box-shadow: 0px 0px 15px transparent
                        border-left: 2px solid $blue
                        border-bottom: 1px solid $border
                        transition: .3s ease-in-out
                        cursor: pointer
                        &.online
                            border-left: 2px solid $blue
                            transition: .3s ease-in-out
                            .class_title
                                span
                                    color: $blue
                                    transition: .3s ease-in-out
                        &.highlighted
                            border-left: 2px solid $orange
                            transition: .3s ease-in-out
                            .class_title
                                span
                                    color: $orange
                                    transition: .3s ease-in-out
                        .class_title
                            display: flex
                            flex-flow: row wrap
                            align-items: center
                            justify-content: space-between
                            span
                                flex: 1 0 10%
                                sf_pro_medium(14px)
                                color: $blue
                                transition: .3s ease-in-out
                                img
                                    filter: invert(1)
                            .class_status
                                flex: 0 0 auto
                                sf_pro_regular(12px)
                                color: $black
                                padding: 5px 15px
                                border: 1px solid $border
                                border-radius: 20px
                                &.full
                                    color: $red
                                    border: 1px solid $red
                        .class_text
                            sf_pro_regular(12px)
                            color: $border_alt2
                            margin-top: 5px
                            transition: .3s ease-in-out
                            &.alternate
                                display: flex
                                flex-flow: row wrap
                                align-items: center
                                justify-content: space-between
                        &:last-child
                            border-bottom: 1px solid transparent
                        &:hover,
                        &:focus,
                        &.active
                            border-left: 2px solid $green
                            box-shadow: 0px 0px 30px rgba(0, 0, 0, 0.1)
                            transition: .3s ease-in-out
                            .class_title
                                span
                                    color: $green
                                    transition: .3s ease-in-out
                            .class_text
                                color: $black
                                transition: .3s ease-in-out
                            &.highlighted
                                border-left: 2px solid $violet
                                transition: .3s ease-in-out
                                .class_title
                                    span
                                        color: $violet
                                        transition: .3s ease-in-out

